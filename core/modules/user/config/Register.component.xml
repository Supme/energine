<?xml version="1.0" encoding="UTF-8" ?>
<configuration>
<methods>
	<method name="main">
		<javascript>
			<object name="Register" />
		</javascript>
		<fields>
		  <field name="u_id"/>
		  <field name="u_name"/>
		  <field name="u_nick"/>
		  <field name="u_fullname"/>
          <field name="u_is_male" type="int"/>        
          <field name="u_birthday"/>
          <field name="u_place"/>                              
          <field name="u_avatar_img"/>          
		</fields>
		<toolbar>
			<control id="register" title="BTN_REGISTER" type="submit"/>
		</toolbar>
        <translations>
            <translation const="TXT_REQUIRED_FIELDS" />
            <translation const="TXT_MALE" />
            <translation const="TXT_FEMALE" />
            <translation const="TXT_REGISTRATION_TEXT" />
        </translations>
	</method>
	<method name="checkLogin">
      <uri_patterns>
           <pattern>/check/</pattern>
       </uri_patterns>
	</method>
	<method name="save">
	    <uri_patterns>
	        <pattern>/save-new-user/</pattern>
	    </uri_patterns>
	</method>
	<method name="success">
	    <uri_patterns>
	        <pattern>/success/</pattern>
	    </uri_patterns>
	</method>
</methods>
</configuration>
