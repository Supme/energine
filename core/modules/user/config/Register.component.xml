<?xml version="1.0" encoding="UTF-8" ?>
<configuration>
<methods>
	<method name="main">
		<javascript>
			<object name="Register" />
		</javascript>
		<fields>
		  <field name="u_id"/>
		  <field name="u_name"/>
		  <field name="u_fullname"/>
          <field name="captcha" type="captcha" title="TXT_ENTER_CAPTCHA"/>
		</fields>
		<toolbar>
			<control id="register" title="BTN_REGISTER" type="submit"/>
		</toolbar>
	</method>
	<method name="checkLogin">
      <uri_patterns>
           <pattern>/check/</pattern>
       </uri_patterns>
	</method>
	<method name="save">
	    <uri_patterns>
	        <pattern>/save-new-user/</pattern>
	    </uri_patterns>
	</method>
	<method name="success">
	    <uri_patterns>
	        <pattern>/success/</pattern>
	    </uri_patterns>
	</method>
</methods>
</configuration>
