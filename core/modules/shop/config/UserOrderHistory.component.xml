<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<methods>
		<method name="main">
			<uri_patterns>
				<pattern>/</pattern>
				<pattern>/page-[int]/</pattern>
			</uri_patterns>
			<fields>
				<field name="order_id"/>
				<field name="order_created" outputFormat="%d/%m/%Y %H:%M" type="datetime"/>
				<field name="os_id"/>
			</fields>
		</method>
		<method name="view" rights="1">
			<uri_patterns>
				<pattern>/[int]/</pattern>
			</uri_patterns>
			<fields>
				<field name="order_id"/>
				<field name="order_created" outputFormat="%d/%m/%Y %H:%M" type="datetime"/>
				<field name="u_id"/>
				<field name="order_address"/>
				<field name="order_contact_person"/>
				<field name="order_delivery_comment"/>
				<field name="os_id"/>
				<field name="order_detail"/>
			</fields>
		</method>
	</methods>
</configuration>