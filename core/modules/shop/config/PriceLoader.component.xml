<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<methods>
		<method name="main">
			<fields>
				<field name="price_loader_file" type="file" title="FIELD_FILE_NAME" />
			</fields>
			<toolbar>
				<control id="load" title="BTN_LOAD" type="submit" onclick="loadFile"/>
			</toolbar>
		</method>
		<method name="loadFile">
			<uri_patterns>
				<pattern>/load/</pattern>
			</uri_patterns>
			<fields>
				<field name="price_loader_result" type="string" mode="1"/>
			</fields>
		</method>
	</methods>
</configuration>