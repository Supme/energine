{
    "name": "energine/cmf",
    "description": "Energine is a content management framework based on the XML/XSLT transformations.",
    "minimum-stability": "stable",
    "license": "MIT",
    "keywords": ["xml", "xslt", "framework", "cmf"],
    "homepage": "http://energine.net/",
    "require": {
        "php": ">=5.4.0",
        "ext-dom":"*",
        "ext-gd":"*",
        "ext-json":"*",
        "ext-libxml":"*",
        "ext-mbstring":"*",
        "ext-PDO":"*",
        "ext-pdo_mysql":"*",
        "ext-SimpleXML":"*",
        "ext-session":"*",
        "ext-tidy":"*",
        "ext-xsl":"*"
    },
    "autoload": {
        "psr-4": {
            "Energine\\": "core/modules",
            "EnergineSite\\": "starter/htdocs/site/modules"
        }
    },
    "authors": [
        {
            "name": "Pavel Dubenko",
            "email": "d.pavka@gmail.com",
            "homepage": "http://web-developer.kiev.ua",
            "role": "Owner"
        },
        {
            "name": "Andy Karpov",
            "email": "andy.karpov@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Andrew Alexeenko",
            "email": "wildex1@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Valeriy Zinchenko",
            "email": "formatcok@gmail.com",
            "role": "Developer"
        }
    ],
    "config": {

    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "energine/cmf",
                "version": "3.1.7",
                "source": {
                    "url": "https://github.com/energine-cmf/energine.git",
                    "type": "git",
                    "reference": "energine.ns"
                }
            }
        }
    ]
}